#include<stdio.h>
#include<conio.h>
int main ()
{
int x ;
printf("enter marks");
scanf("%d",&x);
if((x <= 100 ) & (x>=85))
{
printf("grade A");
}
else if((x <= 84 ) & (x >= 70))
{
printf("grade B");
}
else if((x <= 69) & (x >= 55))
{
printf(" grade C");
}
else if ((x <= 54) & (x >= 40))
{
printf("grade D");
}
else (x < 40)
{
printf("grade F")
}
getch();
}
